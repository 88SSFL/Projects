def consistence(k):
    a=list()
    b=list(k)
    c=['(','[','{',')',']','}']
    d=0
    e=0
    while True:
        if d==len(b):
            break
        elif b[d] in c[:3]:
            a.append(b[d])
        elif b[d] in c[3:] and a[-1]!=c[c.index(b[d])-3]:
            e+=1
            break
        elif b[d] in c[3:] and a[-1]==c[c.index(b[d])-3]:
            a=a[:-1]
        d+=1
    return print('True') if e==0 else print('False')
